  agent {
      label 'master'
      }
  
   options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
   }
                       
   stages {
     
        stage("First step") {
        
      steps {
        sh 'ssh jenkins@192.168.0.115'
      }
      
         }
    }
   }
